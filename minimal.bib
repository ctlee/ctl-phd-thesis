  NUMBER = {13},
  PAGES = {3352--3355},
  TITLE = {{CASTp: Computed Atlas of Surface Topography of proteins}},
  VOLUME = {31},
}

@ARTICLE{Edelsbrunner1983a,
  ABSTRACT = {A generalization of the convex hull of a finite set of points in the plane is introduced and analyzed. This generalization leads to a family of straight-line graphs, " -shapes," which seem to capture the intuitive notions of "fine shape" and "crude shape" of point sets. It is shown that a-shapes are subgraphs of the closest point or furthest point Delaunay triangulation. Relying on this result an optimal algorithm that constructs -shapes is developed.},
