  DATE = {2008-03},
  DOI = {10.1093/bioinformatics/btm626},
  ISSN = {1367-4803},
  JOURNALTITLE = {Bioinformatics},
  KEYWORDS = {Catalytic Domain,Probability,Proteins,Proteins: chemistry,Proteins: physiology},
  NUMBER = {5},
  PAGES = {613--620},
  TITLE = {{Prediction of protein functional residues from sequence by probability density estimation}},
  VOLUME = {24},
}

