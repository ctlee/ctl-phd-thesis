  ISSN = {1553-7358},
  JOURNALTITLE = {PLoS Comput. Biol.},
  KEYWORDS = {Computer Simulation,Cytoplasm,Cytoplasm: chemistry,Escherichia coli,Escherichia coli Proteins,Escherichia coli Proteins: chemistry,Escherichia coli: chemistry,Models,Chemical,Models,Molecular,Protein Conformation,Protein Folding},
  NUMBER = {3},
