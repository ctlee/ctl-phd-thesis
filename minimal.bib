  FILE = {:Users/benjaminrjagger/Library/Application Support/Mendeley Desktop/Downloaded/Shan et al. - 2012 - Oncogenic Mutations Counteract Intrinsic Disorder in the EGFR Kinase and Promote Receptor Dimerization.pdf:pdf},
  ISSN = {00928674},
  JOURNALTITLE = {Cell},
