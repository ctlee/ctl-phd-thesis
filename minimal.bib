  ISSN = {1056-8700},
  JOURNALTITLE = {Annu. Rev. Biophys. Biomol. Struct.},
  KEYWORDS = {Animals,Biological,Computer Simulation,Databases,Factual,Genetic,Genome,Genomics,Genomics: methods,Humans,Models,Molecular,Proteins,Proteins: chemistry},
  NUMBER = {1},
  PAGES = {291--325},
