  FILE = {::},
  ISBN = {0021-9606},
  ISSN = {00219606},
  JOURNALTITLE = {J. Chem. Phys.},
  KEYWORDS = {beta-hairpin,computer-simulation,conformational transitions,dynamics simulations,energy landscape,hybrid monte-carlo,jump-processes,protein-folding kinetics,state models,ww domain},
  NUMBER = {17},
  PAGES = {174105},
