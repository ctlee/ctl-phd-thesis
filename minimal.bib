  JOURNALTITLE = {Proc. Natl. Acad. Sci.},
  KEYWORDS = {Algorithms,Binding Sites,Biophysics,Biophysics: methods,Computational Biology,Computational Biology: methods,Crystallography,Ligands,Models,Molecular,Molecular Conformation,Protein Binding,Protein Conformation,Protein Interaction Mapping,Proteins,Proteins: chemistry,Reproducibility of Results,Software,Statistical,X-Ray,X-Ray: methods},
  NUMBER = {1},
  PAGES = {129--134},
  TITLE = {{A threading-based method (FINDSITE) for ligand-binding site prediction and functional annotation}},
