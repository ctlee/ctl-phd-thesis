  JOURNALTITLE = {Curr. Pharm. Des.},
  KEYWORDS = {Allosteric Regulation,Allosteric Site,Computational Biology,Drug Design,Eph Family,Eph Family: chemistry,Humans,Ligands,Models,Molecular,Peptides,Peptides: chemistry,Peptides: pharmacology,Protein Binding,Proteins,Proteins: chemistry,Receptors,ras Proteins,ras Proteins: chemistry},
  NUMBER = {8},
