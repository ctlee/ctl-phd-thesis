  DOI = {10.1016/j.jtbi.2012.03.005},
  FILE = {:Users/jeffreywagner/Library/Application Support/Mendeley Desktop/Downloaded/Namboodiri et al. - 2012 - Looking for a sequence based allostery definition A statistical journey at different resolution scales.pdf:pdf},
  ISSN = {00225193},
  JOURNALTITLE = {J. Theor. Biol.},
  KEYWORDS = {Allosteric Regulation,Allosteric Site,Allosteric Site: physiology,Allosteric drugs,Amino Acid Sequence,Chemical,Co-evolution,Computational Biology,Computational Biology: methods,Free-energy-transfer,Hydrophobic and Hydrophilic Interactions,Hydrophobicity,Models,Molecular,Protein Binding,Protein Binding: physiology,Proteins,Proteins: chemistry,Recurrence quantification analysis,ras Proteins,ras Proteins: genetics},
