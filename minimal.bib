  DOI = {10.1073/pnas.1104614108},
  FILE = {:Users/benjaminrjagger/Library/Application Support/Mendeley Desktop/Downloaded/Dror et al. - 2011 - Pathway and mechanism of drug binding to G-protein-coupled receptors.pdf:pdf},
  ISSN = {0027-8424},
  JOURNALTITLE = {Proc. Natl. Acad. Sci.},
  NUMBER = {32},
  PAGES = {13118--13123},
