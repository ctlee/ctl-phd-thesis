  DOI = {10.1073/pnas.0711732105},
  ISSN = {0027-8424},
  JOURNALTITLE = {Proc. Natl. Acad. Sci.},
  KEYWORDS = {Crystallography,Kinetics,Ligands,Mutant Proteins,Mutant Proteins: chemistry,Peptides,Peptides: chemistry,Protein Structure,Proteins,Proteins: chemistry,Secondary,Tertiary,Thermodynamics,X-Ray},
  NUMBER = {12},
