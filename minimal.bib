  DOI = {10.1073/pnas.1103547108},
  FILE = {:Users/benjaminrjagger/Library/Application Support/Mendeley Desktop/Downloaded/Buch, Giorgino, De Fabritiis - 2011 - Complete reconstruction of an enzyme-inhibitor binding process by molecular dynamics simulation(2).pdf:pdf;:Users/benjaminrjagger/Library/Application Support/Mendeley Desktop/Downloaded/Buch, Giorgino, De Fabritiis - 2011 - Complete reconstruction of an enzyme-inhibitor binding process by molecular dynamics simulations.pdf:pdf},
  ISBN = {0027-8424},
  ISSN = {0027-8424},
  JOURNALTITLE = {Proc. Natl. Acad. Sci.},
  KEYWORDS = {Animals,Benzamidines,Benzamidines: chemistry,Benzamidines: metabolism,Binding Sites,Cattle,Markov Chains,Models,Molecular,Molecular Dynamics Simulation,Protein Binding,Protein Conformation,Serine Proteinase Inhibitors,Serine Proteinase Inhibitors: chemistry,Serine Proteinase Inhibitors: metabolism,Thermodynamics,Trypsin,Trypsin: chemistry,Trypsin: metabolism},
