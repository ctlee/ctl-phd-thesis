  AUTHOR = {Doman, Thompson N. and McGovern, Susan L. and Witherbee, Bryan J. and Kasten, Thomas P. and Kurumbail, Ravi and Stallings, William C. and Connolly, Daniel T. and Shoichet, Brian K.},
