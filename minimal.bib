  JOURNALTITLE = {Proc. Natl. Acad. Sci.},
  KEYWORDS = {Allosteric Regulation,Allosteric Regulation: genetics,Binding Sites,Binding Sites: genetics,Cell Line,Evolution,Fluorescence,G-Protein-Coupled,G-Protein-Coupled: genetics,G-Protein-Coupled: metabolism,Humans,Models,Molecular,Mutagenesis,Protein Conformation,Psychotropic Drugs,Psychotropic Drugs: metabolism,Receptors,Signal Transduction,Signal Transduction: genetics},
