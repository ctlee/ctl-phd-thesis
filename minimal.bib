  ISSN = {0021-9606},
  JOURNALTITLE = {J. Chem. Phys.},
  KEYWORDS = {Algorithms,Bayes Theorem,Chemistry,Computer Simulation,Computers,Kinetics,Markov Chains,Models,Physical,Physical: methods,Probability,Protein Folding,Software,Statistical,Stochastic Processes,Time Factors},
  NUMBER = {5},
  PAGES = {054112},
  TITLE = {{Efficient Computation of the First Passage Time Distribution of the Generalized Master Equation by Steady-State Relaxation}},
