  ISSN = {1367-4803},
  JOURNALTITLE = {Bioinformatics},
  KEYWORDS = {Algorithms,Animals,Binding Sites,Computational Biology,HSP90 Heat-Shock Proteins,HSP90 Heat-Shock Proteins: chemistry,Models,Molecular,Molecular Dynamics Simulation,Protein Conformation,Proteins,Proteins: chemistry,Software,p38 Mitogen-Activated Protein Kinases,p38 Mitogen-Activated Protein Kinases: chemistry},
  NUMBER = {23},
