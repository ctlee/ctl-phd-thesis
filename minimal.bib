  FILE = {:Users/benjaminrjagger/Library/Application Support/Mendeley Desktop/Downloaded/Yu et al. - 2015 - Full kinetics of CO entry, internal diffusion, and exit in myoglobin from transition-path theory simulations(2).pdf:pdf},
  ISSN = {0002-7863},
  JOURNALTITLE = {J. Am. Chem. Soc.},
  NUMBER = {8},
  PAGES = {3041--3050},
  TITLE = {{Full Kinetics of CO Entry, Internal Diffusion, and Exit in Myoglobin from Transition-Path Theory Simulations}},
