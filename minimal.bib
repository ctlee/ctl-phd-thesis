@ARTICLE{Landau2005,
  ABSTRACT = {Experimental approaches for the identification of functionally important regions on the surface of a protein involve mutagenesis, in which exposed residues are replaced one after another while the change in binding to other proteins or changes in activity are recorded. However, practical considerations limit the use of these methods to small-scale studies, precluding a full mapping of all the functionally important residues on the surface of a protein. We present here an alternative approach involving the use of evolutionary data in the form of multiple-sequence alignment for a protein family to identify hot spots and surface patches that are likely to be in contact with other proteins, domains, peptides, DNA, RNA or ligands. The underlying assumption in this approach is that key residues that are important for binding should be conserved throughout evolution, just like residues that are crucial for maintaining the protein fold, i.e. buried residues. A main limitation in the implementation of this approach is that the sequence space of a protein family may be unevenly sampled, e.g. mammals may be overly represented. Thus, a seemingly conserved position in the alignment may reflect a taxonomically uneven sampling, rather than being indicative of structural or functional importance. To avoid this problem, we present here a novel methodology based on evolutionary relations among proteins as revealed by inferred phylogenetic trees, and demonstrate its capabilities for mapping binding sites in SH2 and PTB signaling domains. A computer program that implements these ideas is available freely at: http://ashtoret.tau.ac.il/ approximately rony},
  AUTHOR = {Landau, Meytal and Mayrose, Itay and Rosenberg, Yossi and Glaser, Fabian and Martz, Eric and Pupko, Tal and Ben-Tal, Nir},
  URL = {http://www.ncbi.nlm.nih.gov/pubmed/11243830 http://nar.oxfordjournals.org/lookup/doi/10.1093/nar/gki370 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1160131{\&}tool=pmcentrez{\&}rendertype=abstract},
  DATE = {2005-07},
  DOI = {10.1093/nar/gki370},
  FILE = {::},
  ISSN = {1362-4962},
  JOURNALTITLE = {Nucleic Acids Res.},
  KEYWORDS = {Algorithms,Amino Acid Substitution,Amino Acids,Amino Acids: chemistry,Animals,Bacterial Proteins,Bacterial Proteins: chemistry,Bayes Theorem,Binding Sites,Evolution,Humans,Internet,Models,Molecular,Phylogeny,Potassium Channels,Potassium Channels: chemistry,Protein Conformation,Proteins,Proteins: chemistry,Proteins: classification,Proteins: physiology,Quality Control,Sequence Homology,Software,Surface Properties,Tyrosine,Tyrosine: chemistry,src Homology Domains},
