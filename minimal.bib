  FILE = {::},
  ISSN = {0027-8424},
  JOURNALTITLE = {Proc. Natl. Acad. Sci.},
  KEYWORDS = {Amino Acyl,Amino Acyl: metabolism,Archaea,Archaea: enzymology,Bacteria,Bacteria: enzymology,Computer Simulation,Glutamate-tRNA Ligase,Glutamate-tRNA Ligase: metabolism,Leucine-tRNA Ligase,Leucine-tRNA Ligase: metabolism,Protein Structure,RNA,Secondary,Transfer,Transfer RNA Aminoacylation},
  NUMBER = {16},
  PAGES = {6620--6625},
  TITLE = {{Dynamical networks in tRNA:protein complexes}},
