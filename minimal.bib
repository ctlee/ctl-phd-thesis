  DOI = {10.1039/c1cp20854c},
  FILE = {:Users/benjaminrjagger/Library/Application Support/Mendeley Desktop/Downloaded/C{\'{e}}zard et al. - 2011 - Molecular dynamics studies of native and substituted cyclodextrins in different media 1. Charge derivation and f.pdf:pdf},
  ISSN = {1463-9076},
  JOURNALTITLE = {Phys. Chem. Chem. Phys.},
  NUMBER = {33},
  PAGES = {15103},
  TITLE = {{Molecular Dynamics Studies of Native and Substituted Cyclodextrins in Different Media: 1. Charge Derivation and Force Field Performances}},
