isbn = {00052736},
issn = {00052736},
journal = {Biochim. Biophys. Acta - Biomembr.},
keywords = {GPCRs,Kinetics,Nanodiscs,Scintillation proximity assay,Surface plasmon resonance},
month = may,
number = {5},
pages = {1224--1233},
pmid = {25725488},
