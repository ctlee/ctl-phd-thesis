doi = {10.1063/1.4891305},
file = {:Users/benjaminrjagger/Library/Application Support/Mendeley Desktop/Downloaded/Cardenas, Elber - 2014 - Modeling kinetics and equilibrium of membranes with fields milestoning analysis and implication to permeatio(3).pdf:pdf},
