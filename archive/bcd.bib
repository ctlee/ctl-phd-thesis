@article{Wong2018,
abstract = {Drug-binding kinetics could play important roles in determining the efficacy of drugs and has caught the attention of more drug designers. Using the dissociation of 1H-pyrrolo[2,3-b]-pyridines from the focal adhesion kinase as an example, this work finds that steered molecular dynamics simulations could help screen compounds with long-residence times. It also reveals a two-step mechanism of ligand dissociation resembling the release of ADP from protein kinase A reported earlier. A phenyl group attaching to the pyrrole prolongs residence time by creating a large activation barrier for transition from the bound to the intermediate state when it becomes exposed to the solvent. Principal component analysis shows that ligand dissociation does not couple with large-scale collective motions of the protein involving many of its amino acids. Rather, a small subset of amino acids dominates. Some of these amino acids do not contact the ligands directly along the dissociation pathways and could exert long-range allosteric effects. {\textcopyright} 2018 Wiley Periodicals, Inc. Steered molecular dynamics simulations show that 1H-pyrrolo[2,3-b]-pyridines dissociate from the focal adhesion kinase through a two-step mechanism, revealed by the time course of different properties such as the solvent-accessible surface area of the compounds. A similar two-step mechanism was found before for the dissociation of ADP from protein kinase A. Many ATP-competitive inhibitors might dissociate from protein kinases through this same mechanism.},
author = {Wong, Chung F.},
doi = {10.1002/jcc.25201},
file = {:Users/benjaminrjagger/Library/Application Support/Mendeley Desktop/Downloaded/Wong - 2018 - Steered molecular dynamics simulations for uncovering the molecular mechanisms of drug dissociation and for drug screening.pdf:pdf},
issn = {01928651},
journal = {J. Comput. Chem.},
keywords = {ATP-competitive inhibitors,Drug-dissociation kinetics,Focal adhesion kinase,Steered molecular dynamics},
month = mar,
Title = {{Steered Molecular Dynamics Simulations for Uncovering the Molecular Mechanisms of Drug Dissociation and for Drug Screening: A Test on the Focal Adhesion Kinase}},
url = {http://doi.wiley.com/10.1002/jcc.25201},
year = {2018}
}
