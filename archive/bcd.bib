file = {:Users/benjaminrjagger/Library/Application Support/Mendeley Desktop/Downloaded/Tang, Chang - Unknown - Systematic Dissociation Pathway Searches Guided by Principal Component Modes.pdf:pdf},
issn = {1549-9618},
