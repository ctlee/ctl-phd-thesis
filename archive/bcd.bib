month = dec,
number = {12},
pages = {2193--2198},
Title = {{Kinetics on Isomeric Alcohols Recognition by $\alpha$- and $\beta$-Cyclodextrins Using Ultrasonic Relaxation Method}},
url = {http://www.journal.csj.jp/doi/10.1246/bcsj.77.2193},
volume = {77},
year = {2004}
}
