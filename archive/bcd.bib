issn = {00981354},
journal = {Comput. Chem. Eng.},
keywords = {AMBER ff03,Hydration free energies,Solvation,Thermodynamic integration,Unnatural amino acids,Water},
pages = {745--752},
publisher = {Elsevier Ltd},
Title = {{Hydration Free Energies Calculated Using the AMBER Ff03 Charge Model for Natural and Unnatural Amino Acids and Multiple Water Models}},
url = {http://dx.doi.org/10.1016/j.compchemeng.2014.07.017},
volume = {71},
year = {2014}
}
@article{Plattner2015a,
abstract = {Understanding the structural mechanisms of protein-ligand binding and their dependence on protein sequence and conformation is of fundamental importance for biomedical research. Here we investigate the interplay of conformational change and ligand-binding kinetics for the serine protease Trypsin and its competitive inhibitor Benzamidine with an extensive set of 150 $\mu$s molecular dynamics simulation data, analysed using a Markov state model. Seven metastable conformations with different binding pocket structures are found that interconvert at timescales of tens of microseconds. These conformations differ in their substrate-binding affinities and binding/dissociation rates. For each metastable state, corresponding solved structures of Trypsin mutants or similar serine proteases are contained in the protein data bank. Thus, our wild-type simulations explore a space of conformations that can be individually stabilized by adding ligands or making suitable changes in protein sequence. These findings provide direct evidence of conformational plasticity in receptors.},
annote = {NULL},
author = {Plattner, Nuria and No{\'{e}}, Frank},
doi = {10.1038/ncomms8653},
file = {:Users/benjaminrjagger/Library/Application Support/Mendeley Desktop/Downloaded/Plattner, No{\'{e}} - 2015 - Protein conformational plasticity and complex ligand-binding kinetics explored by atomistic simulations and Mark.pdf:pdf},
issn = {2041-1723},
journal = {Nat. Commun.},
number = {May},
pages = {7653},
pmid = {26134632},
Title = {{Protein Conformational Plasticity and Complex Ligand-Binding Kinetics Explored by Atomistic Simulations and Markov Models}},
url = {http://www.nature.com/doifinder/10.1038/ncomms8653},
volume = {6},
year = {2015}
}
@article{Huang2018,
abstract = {Through adding a harmonic boost potential to smooth the system potential energy surface, Gaussian accelerated molecular dynamics (GaMD) provides enhanced sampling and free energy calculation of biomolecules without the need of predefined reaction coordinates. This work continues to improve the acceleration power and energy reweighting of the GaMD by combining the GaMD with replica exchange algorithms. Two versions of replica exchange GaMD (rex-GaMD) are presented: force constant rex-GaMD and threshold energy rex-GaMD. During simulations of force constant rex-GaMD, the boost potential can be exchanged between replicas of different harmonic force constants with fixed threshold energy. However, the algorithm of threshold energy rex-GaMD tends to switch the threshold energy between lower and upper bounds for generating different levels of boost potential. Testing simulations on three model systems, including the alanine dipeptide, chignolin, and HIV protease, demonstrate that through continuous exchanges of t...},
