month = oct,
number = {10},
pages = {e1004381},
pmid = {26505480},
Title = {{Multiscale Estimation of Binding Kinetics Using Brownian Dynamics, Molecular Dynamics and Milestoning}},
url = {http://dx.plos.org/10.1371/journal.pcbi.1004381},
volume = {11},
year = {2015}
}
