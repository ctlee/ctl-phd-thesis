Title = {{Scalable Molecular Dynamics with NAMD}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2486339{\&}tool=pmcentrez{\&}rendertype=abstract http://doi.wiley.com/10.1002/jcc.20289},
volume = {26},
