keywords = {Amino Acids,Amino Acids: analysis,Amino Acids: chemistry,Computer Simulation,Cyclohexanes,Cyclohexanes: chemistry,Hydrophobic and Hydrophilic Interactions,Lipid Bilayers,Lipid Bilayers: chemistry,Models, Biological,Water,Water: chemistry},
month = {may},
number = {9},
pages = {3393--404},
pmid = {18212019},
Title = {{Distribution of Amino Acids in a Lipid Bilayer from Computer Simulations.}},
url = {http://www.sciencedirect.com/science/article/pii/S0006349508704193},
volume = {94},
year = {2008}
}

@article{Bauer2011,
abstract = {We investigate permeation energetics of water entering a model dimyristoylphosphatidylcholine (DMPC) bilayer via molecular dynamics simulations using polarizable charge equilibration (CHEQ) models. Potentials of mean force show 4.5-5.5 kcal/mol barriers for water permeation into bilayers. Barriers are highest when water coordination within the bilayer is prevented, and also when using force fields that accurately reproduce experimental alkane hydration free energies. The magnitude of the average water dipole moment decreases from 2.6 D (in bulk) to 1.88 D (in membrane interior). This variation correlates with the change in a water molecule's coordination number. ?? 2011 Elsevier B.V. All rights reserved.},
