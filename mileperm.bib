month = {dec},
number = {3},
pages = {268--278},
pmid = {16797116},
Title = {{Diffusion Constant of K+ Inside Gramicidin A: A Comparative Study of Four Computational Methods}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2266632{\&}tool=pmcentrez{\&}rendertype=abstract},
